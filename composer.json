{
    "name": "suman/egnyte",
    "description": "Egnyte filesystem API with League filesystem",
    "keywords": [
        "Flystem",
        "egnyte"
    ],
    "homepage": "https://github.com/php-loep/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": "suman poudel",
            "email": "suman.poudel2227@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Suman\\Egnyte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Suman\\Egnyte\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
